@{
    Layout="_LayoutMember";
}
<br><br><br><br><br>
<form method="POST" id="form1" enctype="multipart/form-data">
    <label>Category</label>
    <input class="form-control" placeholder="Category" name="category">
    <label>Title</label>
    <input class="form-control" placeholder="Title" name="title">
    <label>Body</label>
    <input class="form-control" placeholder="Body" name="body">
    <label>Image</label>
    <input class="form-control" type="file" name="image">
    <br>
    <button class="btn btn-info" type="submit" asp-action="Add" asp-controller="Member">Send</button>
    <button class="btn btn-info" type="submit" asp-action="AddThread" asp-controller="Member">Add Tread</button>
</form>